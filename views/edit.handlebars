        <form   method="post", action="/editUser/{{data.dataValues.empID}}">
        <h2> Edit Employee ACCOUNT</h3>
         <label  class="mt-4">Name</label>
        <input  class="form-control " type="text" name='name' value="{{data.dataValues.name}}" placeholder="Enter your name"/>
